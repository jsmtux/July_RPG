[sub_resource type="GDScript" id="GDScript_has6b"]
script/source = "extends Node3D

@export var door_is_open = true;

var OPEN_TIME = 1.0;
var CLOSED_TIME = 0.0;

signal door_anmiation_finished

# Called when the node enters the scene tree for the first time.
func _ready():
	set_open(door_is_open)
	$AnimationPlayer.animation_finished.connect(animation_finished_cb)

func set_open(open):
	$AnimationPlayer.assigned_animation = \"door_open\";
	$AnimationPlayer.seek(OPEN_TIME if open else CLOSED_TIME, true);
	door_is_open = open;

func animate_open(open):
	if open:
		$AnimationPlayer.play(\"door_open\")
	else:
		$AnimationPlayer.play_backwards(\"door_open\")
	await door_anmiation_finished
	door_is_open = open;

func animation_finished_cb(anim_name):
	door_anmiation_finished.emit()
"

